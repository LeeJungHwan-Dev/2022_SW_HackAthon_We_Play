<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/naloginbtn"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#30C75A"
        android:text="네이버 로그인"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="536dp" />

    <Button
        android:id="@+id/hmloginbtn"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#CDC5C5"
        android:text="로그인"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="636dp" />

    <Button
        android:id="@+id/ggloginbtn"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:backgroundTint="#4285F4"
        android:text="구글 로그인"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="586dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="453dp"
        android:layout_height="428dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="-21dp"
        tools:layout_editor_absoluteY="76dp" />

</androidx.constraintlayout.widget.ConstraintLayout>